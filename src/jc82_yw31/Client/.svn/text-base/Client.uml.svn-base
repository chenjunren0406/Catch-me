            <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_ljqFNGbdEeOSC5j92XPLhA" name="connect" specification="_liXEsGbdEeOSC5j92XPLhA">
              <ownedParameter xmi:id="_ljqFNWbdEeOSC5j92XPLhA" name="IpAddress">
                <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
              </ownedParameter>
              <language>java</language>
              <body>/*&#xD;
		 * get ServerRegistry&#xD;
		 */&#xD;
		Registry remoteRegistry = rmiUtils.getRemoteRegistry(IpAddress);&#xD;
		try{&#xD;
		/*&#xD;
		 * get serverHost&#xD;
		 */&#xD;
		remote_host = (IHost) remoteRegistry.lookup(IHost.BOUND_NAME);&#xD;
		/*&#xD;
		 * send server ownHost&#xD;
		 */&#xD;
		remote_host.sendLocalHostStub(host_stub);&#xD;
		remote_host.requestInvite(host_stub);&#xD;
		&#xD;
		}catch(Exception e){&#xD;
			e.printStackTrace();&#xD;
		}&#xD;
		&#xD;
	</body>
            </ownedBehavior>
