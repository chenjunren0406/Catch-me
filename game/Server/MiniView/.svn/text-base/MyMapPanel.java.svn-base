package jc82_yw31.Server.MiniView;

import gov.nasa.worldwind.awt.WorldWindowGLCanvas;
import map.MapPanel;

/**
 * this MapPanel is used in our game, the only difference of MyMapPanel and MapPanel is that
 * this one can get wwd
 * @author Junren
 *
 */
public class MyMapPanel extends MapPanel {

	/**
	 * 
	 */
	private static final long serialVersionUID = -2277967126481879376L;
	/**
	 * constructor
	 */
	public MyMapPanel(){
		super();
	}
	
	/**
	 * get wwd
	 * @return wwd
	 */
	public WorldWindowGLCanvas getwwd(){
		return this._wwd;
	}
	
}
